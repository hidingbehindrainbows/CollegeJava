class FirstJava
{
   public static void main(String args[])
   {
      double numbers[] = new double [5];
      double average, sum = 0.0;
      numbers[0] = 1.0;
      numbers[1] = 2.0;
      numbers[2] = 3.0;
      numbers[3] = 4.0;
      numbers[4] = 5.0;
      for(int i = 0;i < 5; i++)
      { 
         sum = sum + numbers[i];
      }
      average = sum/5;
      System.out.println("Average is " + average);
   }
}
      
