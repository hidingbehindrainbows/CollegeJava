class Box
{
      double length, height, width;
	double volume()
	{
		return width*height*length;
	}
}

class JavaClass
{
	public static void main(String args[])
	{
		Box one = new Box();
		one.width = 10;
		one.height = 20;
		one.length = 30;
		System.out.println("The volume is: " + one.volume());
	}
}
